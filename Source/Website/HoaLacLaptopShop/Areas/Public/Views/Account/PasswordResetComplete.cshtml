@{
    ViewData["Title"] = "Enter your new Password";
}
<h1>Reset</h1>
<h4>Password</h4>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="PasswordResetComplete">
            <div class="form-group">
                <label class="control-label form-label">Enter your new password</label>
                <input name="newpass" class="form-control" type="password"/>
            </div>
            <div class="form-group">
                <label class="control-label form-label">Confirm your new password</label>
                <input name="confirmpass" class="form-control" type="password"/>
            </div>
            <div class="form-group">
                <input type="submit" value="Submit" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
